
<launch>


    <!--当开启导航功能时 启用导航算法选择-->


    <!-- global_planner -->



    <!-- local_planner -->

    <!-- 开启teb_local_planner导航算法 -->
    <include file="$(find gust)/launch/gust/teb_local_planner.launch" />

    <!-- 开启dwa_local_planner导航算法-->
    <!-- <include file="$(find gust)/launch/gust/dwa_local_planner.launch" /> -->


    <!-- 发布用于建图、导航的TF关系与小车外形可视化 -->
    <include file="$(find gust)/launch/gust/robot_model_visualization.launch" />


    <!-- 扩张卡尔曼滤波 发布odom_combined到footprint的TF,即小车定位 使用cartographer算法时不使用该滤波算法-->
    <include file="$(find gust)/launch/gust/robot_pose_ekf.launch" />


    <!-- turn on lidar开启思岚雷达  -->
    <!-- <include file="$(find rplidar_ros)/launch/rplidar_s2_b.launch" /> -->


    <!-- 设置需要用于导航的地图  -->
    <arg name="map_file" default="$(find gust)/maps/mymap.yaml"/>

    <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)">
        <param name="frame_id" value="map"/>
    </node> 
    

    <!-- 开启用于导航的自适应蒙特卡洛定位amcl-->   
    <include file="$(find gust)/launch/include/rplidar_amcl.launch" />





</launch>
